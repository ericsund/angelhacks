prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: aws-cpp-sdk-iot
Description: C++ SDK for the AWS iot service
Version: 1.1.12
Cflags: -I${includedir}   -Wall -Werror -pedantic -Wextra -fno-exceptions
Libs: -L${libdir} -laws-cpp-sdk-iot 
Libs.private: -lpthread -lcurl
Requires:  aws-cpp-sdk-core
